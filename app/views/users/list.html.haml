.row-fluid.margined
  .span12
    .box
      %h4 
      .row-fluid.quicklist.fn-suggestions
        -@users.each do |user|
          %a.user.fn-suggestion{ :href => "/actas/#{user.id}" }
            %img.img-rounded{ :src => user.avatar(:medium) }
            %span= user.name
      = will_paginate @users
